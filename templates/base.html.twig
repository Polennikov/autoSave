<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>
        {% block title %}Welcome!{% endblock %}
    </title>
    <!-- Bootstrap CSS (jsDelivr CDN) -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-wEmeIV1mKuiNpC+IOBjI7aAzPcEZeedi5yW5f2yOq55WWLwNGmvvx4Um1vskeMj0" crossorigin="anonymous">
    <!-- Bootstrap Bundle JS (jsDelivr CDN) -->
    <script defer src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-p34f1UUtsS3wqzfto5wAAmdvj+osOnFyQFpp4Ua3gs/ZVWx6oOypYoCJhGGScy+8"
            crossorigin="anonymous"></script>

    <script src="https://code.jquery.com/jquery-3.3.1.js"
            integrity="sha256-2Kok7MbOyxpgUVvAk/HJ2jigOSYS2auK4Pfzbm7uH60=" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://bootstraptema.ru/plugins/2015/bootstrap3/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />
    <script src="https://bootstraptema.ru/plugins/jquery/jquery-1.11.3.min.js"></script>
    <script src="https://bootstraptema.ru/plugins/2015/b-v3-3-6/bootstrap.min.js"></script>

    {# {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %} #}
</head>
<body style="background: #94d6ef">
<div class="container">
    <nav class="navbar navbar-light bg-light">
        <a class="navbar-brand ">
            AutoSave
        </a>
        <div class="auth">
            <a class="course_index btn btn-secondary btn-lg mx-3" href="{{ path('home') }}"> главная</a>
            {% if app.user %}
                {% if is_granted('ROLE_AGENT') %}
                    <a class="profile btn btn-secondary btn-lg mx-3" href="{{ path('dtp') }}"> ДТП</a>
                {% else %}
                    <a class="profile btn btn-secondary btn-lg mx-3" href="{{ path('profile') }}"> профиль</a>

                {% endif %}
                <a class="lessonShow btn btn-secondary btn-lg mx-3" href="{{ path('app_logout') }}"> выход</a>

            {% else %}
                <a class="register btn btn-secondary btn-lg mx-3" href="{{ path('register') }}">регистрация</a>
                <a class="lessonShow btn btn-secondary btn-lg mx-3" href="{{ path('app_login') }}"> вход</a>
            {% endif %}

        </div>
    </nav>

    {% block body %}
    {% endblock %}
</div>
</body>
</html>
